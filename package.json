{
  "name": "g2i-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "assets:dev": "cp -R public/* dev/",
    "assets:build": "cp -R public/* build/",
    "html:dev": "pug src/html -o dev -w",
    "html:build": "pug src/html -o build",
    "bundle:dev": "webpack-dev-server",
    "bundle:build": "webpack",
    "prestart": "rm -rf dev && mkdir dev",
    "start": "npm-run-all --parallel html:dev assets:dev bundle:dev",
    "prebuild": "rm -rf build && mkdir build",
    "build": "npm-run-all --parallel html:build assets:build bundle:build",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "webpack:profile": "webpack --profile --json --output-filename stats.json"
  },
  "dependencies": {
    "nock": "^9.1.6",
    "prop-types": "^15.6.0",
    "ramda": "^0.23.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.2.0",
    "styled-components": "^3.0.2",
    "superagent": "^3.5.2",
    "webpack-dev-server": "^2.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "^7.0.0-beta.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.20.0",
    "eslint": "3.16.1",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.64.0",
    "jest": "^22.1.4",
    "less": "^2.7.2",
    "npm-run-all": "^4.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "redux-mock-store": "^1.4.0",
    "webpack": "^2.4.1",
    "webpack-merge": "^4.1.0"
  }
}
